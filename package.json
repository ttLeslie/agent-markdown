{
  "name": "useAgent-markdown",
  "description": "A Vue3 component based on markdown-it, enabling developers to rapidly build enterprise - level large - model applications.",
  "version": "1.0.0",
  "packageManager": "pnpm@9.15.9",
  "author": "jingtaolai <1278207976@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/ttLeslie/useAgent-markdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttLeslie/useAgent-markdown"
  },
  "bugs": "https://github.com/ttLeslie/useAgent-markdown/issues",
  "keywords": [
    "vue3",
    "component",
    "markdown",
    "markdown-it",
    "markdown-it-emoji",
    "Agent",
    "typescript"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:docs": "pnpm -F @agent/docs run dev",
    "dev:play": "pnpm -F @agent/playground run dev",
    "build": "turbo run build",
    "build:docs": "pnpm -F @agent/docs run build",
    "build:gulp": "gulp -f build/gulpfile.js",
    "publish": "turbo run publish",
    "lint:eslint": "eslint --max-warnings 0 \"**/*.{ts,tsx,js,jsx,cjs,mjs,vue}\" --fix",
    "lint:format": "prettier --write \"**/*.{js,jsx,cjs,ts,tsx,mjs,mts,md,vue,scss,css,less,html,json}\"",
    "lint:style": "stylelint \"**/*.{css,scss,less}\" --fix",
    "lint:all": "pnpm run lint:eslint && pnpm run lint:style && pnpm run lint:format",
    "clean": "turbo run clean --continue && rimraf .turbo dist && rm -rf node_modules",
    "changeset:version": "pnpm changeset && pnpm changeset version",
    "deps:update": "pnpm update -r --latest",
    "deps:check": "pnpm outdated -r",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "turbo run build",
    "prepare": "husky install",
    "rename-pkg": "bash ./scripts/rename-package.sh"
  },
  "devDependencies": {
    "@changesets/cli": "catalog:",
    "@commitlint/cli": "catalog:",
    "@agent/commitlint-config": "workspace:*",
    "@agent/eslint-config": "workspace:*",
    "@agent/prettier-config": "workspace:*",
    "@agent/stylelint-config": "workspace:*",
    "@agent/typescript-config": "workspace:*",
    "agent-markdown-vue": "workspace:*",
    "eslint": "catalog:",
    "husky": "catalog:",
    "lint-staged": "catalog:",
    "prettier": "catalog:",
    "rollup": "catalog:",
    "stylelint": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vue": "catalog:",
    "rimraf": "catalog:"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "search-insights"
      ]
    }
  }
}
