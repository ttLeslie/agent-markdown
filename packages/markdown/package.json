{
  "name": "@AgentMarkdown/markdown",
  "description": "@AgentMarkdown markdown library",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf dist && vite build",
    "clean": "rimraf .turbo node_modules dist",
    "publish": "pnpm publish"
  },
  "devDependencies": {
    "@AgentMarkdown/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "sass": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vue": "catalog:",
    "@mdit/plugin-katex": "catalog:",
    "@types/markdown-it": "catalog:",
    "@types/markdown-it-emoji": "catalog:",
    "markdown-it": "catalog:",
    "markdown-it-emoji": "catalog:",
    "es-toolkit": "catalog:",
    "highlight.js": "catalog:",
    "dompurify": "catalog:",
    "github-markdown-css": "catalog:"
  },
  "peerDependencies": {
    "vue": "catalog:"
  }
}
