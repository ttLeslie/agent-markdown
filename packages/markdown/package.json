{
  "name": "agent-markdown-vue",
  "description": "@agent markdown library",
  "homepage": "https://github.com/ttLeslie/agent-markdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttLeslie/agent-markdown"
  },
  "version": "1.2.2",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "vue3",
    "component",
    "markdown",
    "markdown-it",
    "markdown-it-emoji",
    "Agent",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./style.css": "./dist/style.css",
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rimraf dist && vite build",
    "clean": "rimraf .turbo node_modules dist",
    "publish": "pnpm publish",
    "pub": "node ./publish.js"
  },
  "devDependencies": {
    "@agent/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "sass": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vue": "catalog:",
    "@mdit/plugin-katex": "catalog:",
    "@types/markdown-it": "catalog:",
    "@types/markdown-it-emoji": "catalog:",
    "markdown-it": "catalog:",
    "markdown-it-emoji": "catalog:",
    "es-toolkit": "catalog:",
    "highlight.js": "catalog:",
    "dompurify": "catalog:"
  },
  "peerDependencies": {
    "vue": "catalog:"
  }
}